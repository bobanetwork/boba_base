{
  "name": "boba_bridges",
  "version": "0.1.0",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "prepare:bobabase": "mustache config/bobabase.json subgraph.template.yaml > subgraph.yaml",
    "prepare:bobabeam": "mustache config/bobabeam.json subgraph.template.yaml > subgraph.yaml",
    "deploy:subgraph:bobabase": "graph deploy --studio boba-network-boba-base",
    "deploy:subgraph:bobabeam": "graph deploy --studio boba-network-boba-beam"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.29.0",
    "@graphprotocol/graph-ts": "^0.26.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "3.4.1",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "mustache": "^4.2.0",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4"
  }
}
